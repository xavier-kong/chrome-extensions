{
    "name": "Hide Instagram Explore",
    "description": "Hiding Instagram's Most Addictive Feature",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "webNavigation",
        "storage",
        "webRequest",
        "scripting"
    ],
    "content_scripts": [
        { "matches": ["*://www.instagram.com/*"], "js": ["inject.js"] }
    ],
    "host_permissions": ["*://www.instagram.com/*"]
}
