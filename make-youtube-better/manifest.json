{
    "name": "Make YouTube Better",
    "description": "making YouTube better than the original product!",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["*://www.youtube.com/*"],
            "css": ["inject.css"]
        }
    ],
    "permissions": [
        "tabs",
        "webNavigation",
        "storage",
        "webRequest",
        "scripting"
    ],
    "host_permissions": ["*://www.youtube.com/*"],
    "background": {
        "service_worker": "background.js"
    }
}
