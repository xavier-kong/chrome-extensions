{
  "name": "Hide YouTube Search Bar",
  "description": "Hide the youtube search bar!",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/badurl/*"],
      "js": ["myscript.js"]
    }
  ],
  "permissions": [
    "tabs",
    "webNavigation",
    "storage"
  ]
}