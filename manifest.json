{
    "name": "Hide YouTube Search Bar",
    "description": "Hide the youtube search bar!",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["*://www.youtube.com/*"],
            "js": ["myscript.js"],
            "css": ["inject.css"]
        }
    ],
    "permissions": ["tabs", "webNavigation", "storage", "webRequest"],
    "host_permissions": ["*://www.youtube.com/*"],
    "background": {
        "service_worker": "background.js"
    }
}
