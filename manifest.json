{
    "name": "Hide YouTube Search Bar",
    "description": "Hide the youtube search bar!",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["*://www.youtube.com/*"],
            "js": ["myscript.js"]
        }
    ],
    "permissions": ["tabs", "webNavigation", "storage"],
    "background": {
        "service_worker": "background.js"
    }
}
